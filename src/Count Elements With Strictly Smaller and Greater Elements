class Solution {
    public int countElements(int[] nums) {
        Arrays.sort(nums);
       int c=0;
        int mini= nums[0];
        int maxi= nums[nums.length-1];
        
        for(int i=1;i<nums.length-1;i++){
            if(nums[i]>mini && nums[i]<maxi){
                c++;
            }
        }
        return c;
    }
}
