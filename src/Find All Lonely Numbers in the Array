class Solution {
public:
    vector<int> findLonely(vector<int>& nums) {
        vector<int> ans;
        map<int,int> mp;
        for(auto i: nums){
            mp[i]++;
        }
        for(auto i: nums){
            if(mp[i]==1 and mp.find(i+1)==mp.end() and mp.find(i-1)==mp.end()){
                ans.push_back(i);
            }
        }
        return ans;
    }
};
